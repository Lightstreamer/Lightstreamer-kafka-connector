<adapters_conf id="KAFKA">
    <metadata_adapter_initialised_first>N</metadata_adapter_initialised_first>
    <metadata_provider>
        <adapter_class>com.lightstreamer.adapters.metadata.LiteralBasedProvider</adapter_class>
        <!--<classloader>log-enabled</classloader>-->
    </metadata_provider>


    <data_provider>

        <param name="field.ask">${VALUE.ask}</param>
        <param name="field.bid">${VALUE.bid}</param>
        <param name="field.last">${VALUE.last}</param>
        <param name="field.time">${VALUE.time}</param>
        <param name="field.change">${VALUE.change}</param>

        <param name="field.key">${KEY}</param>
        <param name="field.value">${VALUE}</param>
   
    </data_provider>
</adapters_conf>
